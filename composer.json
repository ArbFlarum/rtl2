{
    "name": "flarumarabia/flarum-ext-arabic",
    "type": "flarum-extension",
    "description": "Arabic language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.5",
		"flarumarabia/flarum-ext-rtl":"*@dev",
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Arabic",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "ar",
            "title": "Arabic"
        }
    }
}
